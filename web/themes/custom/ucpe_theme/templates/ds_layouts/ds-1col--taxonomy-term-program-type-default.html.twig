{#
/**
 * @file
 * Display Suite 1 column template.
 *
 * Available variables:
 * - ds_content_wrapper: wrapper around content
 * - attributes: content region attributes
 * - ds_content: content region
 */
#}
<div class="card">
  <div class="card_img">{{ ds_content.field_taxonomy_image|field_value }}</div>
  <div class="card_content">
    <h3>{{ ds_content.field_program_detail_page|field_value }}</h3>
    {{ ds_content.description|field_value }}
  </div>
</div>
